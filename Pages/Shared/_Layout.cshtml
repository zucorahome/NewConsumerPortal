<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - New_ConsumerPortal</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/style.css" />
    <!--Font awesome-->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta2/css/all.min.css" integrity="sha512-YWzhKL2whUzgiheMoBFwW8CKV4qpHQAEuvilg9FAn5VJUDwKZZxkJNuGM4XkWuk94WCrrwslk8yWNGmY1EduTA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-page="/Index">New_ConsumerPortal</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Index">Home</a>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link text-dark dropdown-toggle" href="#" id="loginPageDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">Consumer Portal Pages</a>
                            <ul class="dropdown-menu" aria-labelledby="loginPageDropdown">
                                <li class="">
                                    <a href="/login" class="dropdown-item">Login</a>
                                </li>
                                <li class="">
                                    <a href="/create-account" class="dropdown-item">Create account</a>
                                </li>
                                <li class="">
                                    <a href="/change-password" class="dropdown-item">Change password</a>
                                </li>
                                <li class="">
                                    <a href="/set-password" class="dropdown-item">Set password</a>
                                </li>
                                <li class="">
                                    <a href="/request-reset-password" class="dropdown-item">Request Reset Password</a>
                                </li>
                                <li class="">
                                    <a href="/my-plans" class="dropdown-item">My plans</a>
                                </li>
                            </ul>
                        </li>
                       
                        <li class="nav-item dropdown">
                            <a class="nav-link text-dark dropdown-toggle" href="#" id="marketPartnerDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">Market Partner Pages</a>
                            <ul class="dropdown-menu" aria-labelledby="marketPartnerDropdown">
                                <li class="">
                                    <a href="/MarketPartner/partner-profile" class="dropdown-item">Market Partner Profile</a>
                                </li>
                                <li class="">
                                    <a href="/MarketPartner/plan-registration" class="dropdown-item">Plan Registration</a>
                                </li>
                                <li class="">
                                    <a href="/MarketPartner/product-orders" class="dropdown-item">Product Orders</a>
                                </li>
                                <li class="">
                                    <a href="/MarketPartner/smart-ship" class="dropdown-item">Smart Ship</a>
                                </li>
                            </ul>
                        </li>
                        
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container-fluid g-md-0 mt-2 mt-md-0">
        <main role="main" class="">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - New_ConsumerPortal - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
